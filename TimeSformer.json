{
  "model": {
    "name": "TimeSformer",
    "dim": 512,
    "patch_size": 16,
    "depth": 12,
    "heads": 8,
    "attn_dropout": 0.2,
    "ff_dropout": 0.2,
    "rotary_emb": true,
    "shift_tokens": true
  },
  "train": {
    "epochs": 500,
    "learning_rate": 0.001,
    "save_dir": "checkpoints/TimeSformer",
    "batch_size": 8,
    "resume_training": true
  },
  "dataloader": {
    "dataset_json": "dataset.json",
    "width": 224,
    "height": 224,
    "frames": 8,
    "mode": "uniform",
    "stride": 0,
    "frame_strategy": "zero_pad",
    "train_transform": "none",
    "val_transform": "none"
  }
}